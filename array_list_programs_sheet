l1 = [11,20,33,40,55,30,1]
n = 31   

def myfunc(l1,n):
  l2 = []
  for i in range(0,len(l1)):
    for j in range(i+1,len(l1)):
      if l1[i] + l1[j] == n:
        l2.append((l1[i],l1[j]))
  return l2
        
  return "Please enter valid sum number."
print(myfunc(l1,n))
output:
[(11, 20), (30, 1)]

